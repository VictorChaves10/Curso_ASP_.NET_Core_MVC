@model LancheListViewModel

@{
    ViewData["Title"] = "List";
}

<section>
    
    <div class="m-5">
        <h1 class="text-center">@Model.CategoriaAtual</h1>
    </div>

    <div class="container-xl">
        <div class="row justify-content-center gap-1">
            @foreach (var lanche in Model.Lanches)
            {
                <partial name="_LancheResumo" for="@lanche" />
            }
        </div>
    </div>

</section>
